<!DOCTYPE html>
<html>
  <head>
    <title>MyBook</title>
    <meta charset="utf-8">
    <!-- <link rel="stylesheet" href="node_modules/flip-book/css/font-awesome.min.css"> -->
  </head>
  <body>
    <style type="text/css">
      body {
        background-color: #333;
        margin: 0;
        padding: 0;
      }
      .container {
        height: 95vh;
        width: 95%;
        margin: 20px auto;
        /*border: 2px solid red;
        box-shadow: 0 0 5px red;*/
      }
      .fullscreen {
        background-color: #333;
      }
      .demo-msg{
        display: none;
      }
    </style>
    <div class="container" id="container"></div>
    <script>
      var intervalId = setInterval( findIframe, 100);
      function findIframe(){
        var iframe = document.querySelector("iframe");
        if (iframe){
          var contentLoaded = iframe.contentWindow.document.querySelector(".demo-msg");
          if (contentLoaded){
            contentLoaded.style.display = "none";
            clearInterval(intervalId);
          }
        }
      }
    </script>
  </body>
</html>
